{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "main": "dist/src/index.js",
  "engines": {
    "node": ">= 14",
    "npm": ">= 8"
  },
  "scripts": {
    "build": "npm run codegen && npm run prisma:generate && tsc",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch ./schema.graphql",
    "codegen": "graphql-codegen --config codegen.ts",
    "dev:watch": "nodemon src/index.ts",
    "lint": "eslint src --fix",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "start": "concurrently --kill-others --names \"node,codegen\" -c \"cyan,green\" \"npm run dev:watch\" \"npm run codegen:watch\"",
    "test:watch": "jest --watchAll",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node src/prisma/seed.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-document-nodes": "3.0.0",
    "@graphql-codegen/typescript-resolvers": "3.0.0",
    "@types/cors": "2.8.13",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/jest": "29.4.0",
    "@types/lodash": "4.14.191",
    "@types/node": "18.13.0",
    "casual": "1.6.2",
    "concurrently": "7.6.0",
    "jest": "29.4.3",
    "nodemon": "2.0.20",
    "prisma": "4.11.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@apollo/server": "4.5.0",
    "@apollo/server-plugin-response-cache": "4.1.0",
    "@prisma/client": "4.11.0",
    "@sentry/node": "7.37.2",
    "awesome-phonenumber": "5.1.0",
    "axios": "1.3.3",
    "body-parser": "1.20.2",
    "bson": "5.0.1",
    "cloudinary": "1.34.0",
    "cors": "2.8.5",
    "dataloader": "2.2.2",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "firebase-admin": "11.5.0",
    "graphql": "16.6.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-query-complexity": "0.12.0",
    "graphql-rate-limit-directive": "2.0.3",
    "graphql-tag": "2.12.6",
    "graphql-tools": "8.3.18",
    "lodash": "4.17.21",
    "zod": "3.20.6"
  }
}
